[required_features=NUMERIC_TYPE]
[name=round_numeric_with_no_rounding_mode]
SELECT ROUND(NUMERIC "2.45", 1);
--
ARRAY<STRUCT<NUMERIC>>[{2.5}]
==

[default required_features=ROUND_WITH_ROUNDING_MODE,NUMERIC_TYPE]
[name=round_numeric_with_round_half_even]
SELECT ROUND(NUMERIC "2.45", 1, "ROUND_HALF_EVEN");
--
ARRAY<STRUCT<NUMERIC>>[{2.4}]
==

[name=round_numeric_with_round_half_even_unaffected]
SELECT ROUND(NUMERIC "2.45", 5, "ROUND_HALF_EVEN");
--
ARRAY<STRUCT<NUMERIC>>[{2.45}]
==

[name=round_numeric_with_round_half_away_from_zero]
SELECT ROUND(NUMERIC "2.45", 1, "ROUND_HALF_AWAY_FROM_ZERO");
--
ARRAY<STRUCT<NUMERIC>>[{2.5}]
==

[name=round_numeric_with_round_half_away_from_zero_unaffected]
SELECT ROUND(NUMERIC "2.45", 12, "ROUND_HALF_AWAY_FROM_ZERO");
--
ARRAY<STRUCT<NUMERIC>>[{2.45}]
==

[name=round_numeric_with_null_rounding_mode]
SELECT ROUND(NUMERIC "2.45", 1, NULL);
--
ARRAY<STRUCT<NUMERIC>>[{NULL}]
==

[name=round_numeric_passing_rounding_mode_as_cast_expression]
SELECT ROUND(NUMERIC "2.45", 1, CAST(concat("ROUND_", "HALF_AWAY_FROM_ZERO") as ROUNDING_MODE));
--
ARRAY<STRUCT<NUMERIC>>[{2.5}]
==

[name=round_numeric_null_rounding_mode_via_casting]
SELECT ROUND(NUMERIC "2.45", 1, CAST(NULL as ROUNDING_MODE));
--
ARRAY<STRUCT<NUMERIC>>[{NULL}]
==


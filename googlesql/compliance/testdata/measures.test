[name=measure_propagation_past_left_outer_join]
[required_features=ENABLE_MEASURES,MULTILEVEL_AGGREGATION,AGGREGATE_FILTERING,NULL_HANDLING_MODIFIER_IN_AGGREGATE]
SELECT
  AGG(T2.measure_country_count),
  COUNT(* GROUP BY T2.country),
  AGG(T2.measure_array_agg_nullable_str_respect_nulls),
  ARRAY_AGG(T2.nullable_str RESPECT NULLS)
FROM (
  SELECT *
  FROM MeasureTable_SingleKey
) AS T1 LEFT OUTER JOIN (
  SELECT *
  FROM MeasureTable_SingleKey
  WHERE key <= 5
) AS T2
ON T1.key = T2.key

[prepare_database]
CREATE TABLE KeyValue AS
SELECT 1 AS Key, TRUE AS Value
  UNION ALL
  SELECT 2 as Key, NULL as Value
--
ARRAY<STRUCT<Key INT64, Value BOOL>>[{1, true}, {2, NULL}]
==
[prepare_database]
CREATE TABLE TestTable AS
SELECT 1 AS id, TRUE AS bool_val, 'a' AS str_val
  UNION ALL
  SELECT 2, NULL, 'b'
  UNION ALL
  SELECT 3, NULL, 'c'
  UNION ALL
  SELECT 4, NULL, NULL
--
ARRAY<STRUCT<id INT64, bool_val BOOL, str_val STRING>>[
  {1, true, "a"},
  {2, NULL, "b"},
  {3, NULL, "c"},
  {4, NULL, NULL}
]
==
[name=is_unknown_1]
SELECT NULL IS UNKNOWN
--
ARRAY<STRUCT<BOOL>>[{true}]
==
[name=is_unknown_2]
SELECT NULL IS NOT UNKNOWN
--
ARRAY<STRUCT<BOOL>>[{false}]
==
[name=is_unknown_3]
SELECT TRUE IS UNKNOWN
--
ARRAY<STRUCT<BOOL>>[{false}]
==
[name=is_unknown_4]
SELECT TRUE IS NOT UNKNOWN
--
ARRAY<STRUCT<BOOL>>[{true}]
==
[name=is_unknown_5]
SELECT FALSE IS UNKNOWN
--
ARRAY<STRUCT<BOOL>>[{false}]
==
[name=is_unknown_6]
SELECT FALSE IS NOT UNKNOWN
--
ARRAY<STRUCT<BOOL>>[{true}]
==
[name=is_unknown_7]
SELECT FALSE IS NOT UNKNOWN UNKNOWN
--
ARRAY<STRUCT<UNKNOWN BOOL>>[{true}]
==
[name=is_unknown_8]
SELECT Value IS UNKNOWN
FROM KeyValue kv
--
ARRAY<STRUCT<BOOL>>[unknown order:{true}, {false}]
==
[name=is_unknown_9]
SELECT *
FROM KeyValue kv
WHERE kv.Value IS NOT UNKNOWN
--
ARRAY<STRUCT<Key INT64, Value BOOL>>[{1, true}]
==
[name=is_unknown_10]
SELECT *
FROM TestTable t
WHERE t.bool_val IS UNKNOWN and t.str_val IS NOT NULL
--
ARRAY<STRUCT<id INT64, bool_val BOOL, str_val STRING>>[unknown order:
  {2, NULL, "b"},
  {3, NULL, "c"}
]

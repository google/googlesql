# Test more valid MEASURE property expressions.
[language_features=NONE,+SQL_GRAPH_DEFAULT_LABEL_AND_PROPERTY_DEFINITION_OPTIONS,+SQL_GRAPH_MEASURE_DDL,+MULTILEVEL_AGGREGATION,+AGGREGATE_FILTERING,+NULL_HANDLING_MODIFIER_IN_AGGREGATE,+LIMIT_IN_AGGREGATE]
CREATE PROPERTY GRAPH catalog.aml
NODE TABLES (
  person
    DEFAULT LABEL PROPERTIES (id)
    LABEL no_properties NO PROPERTIES,
  account PROPERTIES (id)
)
EDGE TABLES (
  PersonOwnAccount KEY(personId, accountId)
    SOURCE KEY(personId) REFERENCES person(id)
    DESTINATION KEY(accountId) REFERENCES account(id)
    DEFAULT LABEL PROPERTIES (personId, accountId)
    LABEL test
    PROPERTIES (
      personId as personId,   -- property with form "x as x" is OK.
      MEASURE(1 + 1) as m1,   -- constant expression.
      MEASURE(SUM(1)) as m2,  -- aggregate of constant.
      MEASURE(ANY_VALUE(personId)) as m3, -- any_value().
      MEASURE(SUM(personId) / COUNT(1) * 100) as m4,  -- expressions of aggregates.
      MEASURE(ARRAY_AGG(personId)) as m5, -- array_agg()
      MEASURE(SUM(1 GROUP BY accountId)) as m6, -- multi-level aggregation.
      MEASURE(SUM(SUM(1) group by accountId)) as m7,
      MEASURE(ARRAY_AGG(
        DISTINCT personId
        IGNORE NULLS LIMIT 1)) as m8 -- DISTINCT, IGNORE NULL, and LIMIT clauses.
    )
)
